<UserControl
    x:Class="WpfAppExe.Views.UserControls.RectangleUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfAppExe.Views.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="120" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" />
        <Grid
            Grid.Row="1"
            Margin="0,30,0,0"
            ShowGridLines="True">
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Rectangle
                Grid.Row="0"
                Width="40"
                Height="20"
                Fill="AliceBlue"
                Stroke="Red"
                StrokeThickness="1"
                 RadiusX="10"
                 RadiusY="1"/>
        </Grid>
    </Grid>
</UserControl>
