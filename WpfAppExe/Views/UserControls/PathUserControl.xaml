<UserControl
    x:Class="WpfAppExe.Views.UserControls.PathUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfAppExe.Views.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Path
            Grid.Row="0"
            Data="M 30,0 L 0,45 60,45 Z M 100,0 L 70,45 130,45 Z"
            Stroke="AliceBlue" />
        <Path
            Grid.Row="1"
            Data="M 100,0 H 30 V50"
            Stroke="Black" />
        <Path
            Grid.Row="2"
            Data="M 0,0 C 250,0 50,200 300,200"
            Stroke="Bisque" />
        <Path
            Grid.Row="3"
            Data="M 0,0 C 20,20 30,80 60,0"
            Stroke="Bisque" />
    </Grid>
</UserControl>
